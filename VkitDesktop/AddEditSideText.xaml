<Window x:Class="VkitDesktop.AddEditSideText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add or Edit Side-Text" Height="114" Width="419">
    <Grid>
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,0,10,5" Name="myCancelButton" VerticalAlignment="Bottom" Width="75" Click="myCancelButton_Click" />
        <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,95,5" Name="myOkButton" VerticalAlignment="Bottom" Width="75" Click="myOkButton_Click" />
        <TextBox Height="23" HorizontalAlignment="Stretch" Margin="10,12,10,0" Name="myCardText" VerticalAlignment="Top" />
    </Grid>
</Window>
